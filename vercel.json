{
    "version": 2,
    "env": {
        "DATABASE_URL": "@shop_api_mongo_uri"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
    "rewrites": [
        {
            "source": "@app/(.*)",
            "destination": "<destination>/app/$1"
        },
        {
            "source": "@infra/(.*)",
            "destination": "<destination>/infra/$1"
        }
    ]
}