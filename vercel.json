{
    "version": 2,
    "env": {
        "DATABASE_URL": "@shop_api_mongo_uri"
    },
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "HEAD",
                "OPTIONS"
            ]
        }
    ]
}